[Unit]
Description=Application launch booster for Qt5
After=dbus.socket
Requires=dbus.socket
Wants=booster-qt6-signal.service

[Service]
Type=notify
ExecStart=/usr/libexec/mapplauncherd/booster-qt6 --systemd --boot-mode
Restart=always
RestartSec=1
OOMScoreAdjust=-250

[Install]
WantedBy=user-session.target
